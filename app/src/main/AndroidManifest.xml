<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testapp">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BroadcastReceiverActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IntentServiceActivity">
        </activity>
        <activity android:name=".NotificationActivity"></activity>
        <activity android:name=".SharedPrefsActivity" />
        <activity android:name=".NavigationDrawerActivity" />
        <activity android:name=".ViewPagerAndTabLayout2Activity" />
        <activity android:name=".SpinnersActivity" />
        <activity android:name=".DialogsActivity" />
        <activity android:name=".ToolbarMenuActivity" />
        <activity android:name=".PermissionsActivity" />
        <activity android:name=".ImplicitIntentsActivity" />
        <activity android:name=".MainActivity" />

        <service android:name=".others.MyIntentService" />
    </application>

</manifest>